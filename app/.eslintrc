{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "react/no-unescaped-entities": 0,
    "react/prop-types": 0,
  },
  "globals": {
    "document": true,
    "window": true
  },
  "settings": {
    "import/extensions": [".js",".jsx",".ts",".tsx"],
    "import/parsers": {
      "@typescript-eslint/parser": [".ts",".tsx"]
     },
     "import/resolver": {
         "node": {
             "extensions": [".js",".jsx",".ts",".tsx"]
         }
     }
  }
}